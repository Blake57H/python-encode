        {
          "param": "scale",
          "value": "-1:720:spline+accurate_rnd+full_chroma_int"
        }
      ]
    },
    "color_range": 1,
    "color_primaries": 1,
    "color_trc": 1,
    "colorspace": 1,
    "attachment_copy": true
  },
  "naming": "[src={group}] {episode_name} ({resolution}p) [H265-10bit] [{crc}]"
