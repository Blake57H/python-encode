{
  "encode_settings": {
    "audio_bitrate": 96,
    "audio_codec": "libopus",
    "resolution_height": 720,
    "video_codec": "libaom-av1",
    "video_quality": 24.2,
    "preset": "slow",
    "video_profile": "main",
    "pix_fmt": "yuv420p",
    "vf": {
      "vf_spliter": ",",
      "vf_name": "vf",
      "vf_value": [
        {
          "param": "smartblur",
          "value": "1.5:-0.35:-3.5:0.65:0.25:2.0"
        },
        {
          "param": "scale",
          "value": "-1:720:spline+accurate_rnd+full_chroma_int"
        }
      ]
    },
    "color_range": 1,
    "color_primaries": 1,
    "color_trc": 1,
    "colorspace": 1,
    "attachment_copy": true
  },
  "naming": "[src={group}] {episode_name} ({resolution}p) [AV1-8bit] [{crc}]"
}
